PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dataset: <http://www.esblurock.info/dataset#>
PREFIX ssn: <http://www.w3.org/ns/ssn#>
PREFIX qudt: <http://data.nasa.gov/qudt/owl/qudt#>
PREFIX unit: <http://data.nasa.gov/qudt/owl/unit#>
PREFIX skos:  <http://www.w3.org/2004/02/skos/core#>
SELECT  ?top ?sub  ?object 
		        WHERE {
			dataset:HeatFluxBurner rdfs:subClassOf ?sub .
		 	?sub  owl:onProperty <http://www.w3.org/2004/02/skos/core#related> .
			 ?sub owl:onClass ?object .
			?obj ?pred1 ?sub .
		 	 ?obj <http://purl.org/dc/elements/1.1/type> <http://www.w3.org/ns/ssn/hasOutput>
			                   }