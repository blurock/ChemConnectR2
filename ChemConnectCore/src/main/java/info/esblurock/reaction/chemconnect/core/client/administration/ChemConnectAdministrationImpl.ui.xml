<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style gss="true">
	body {
		background: #e9e9e9;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialModal ui:field="okmodal" type="DEFAULT" 
			inDuration="500" outDuration="500">
			<m:MaterialModalContent>
				<m:MaterialTitle ui:field="title"/>
			</m:MaterialModalContent>
			<m:MaterialModalFooter ui:field="footer">
				<m:MaterialLink ui:field="close" type="FLAT" />
				<m:MaterialLink ui:field="ok" type="FLAT" />
			</m:MaterialModalFooter>
		</m:MaterialModal>
		<m:MaterialHeader ui:field="header">
			<m:MaterialNavBar activates="sidenavFixed"
				backgroundColor="BLUE" shadow="1">
				<m:MaterialNavBrand ui:field="navtitle"
					paddingLeft="60" text="gwt-material" />
			</m:MaterialNavBar>
		</m:MaterialHeader>

		<m:MaterialSideNav m:id="sidenavFixed" ui:field="sideNav"
			width="300">
			<ma:sideprofile.MaterialSideProfile
				url="http://static1.squarespace.com/static/51609147e4b0715db61d32b6/521b97cee4b05f031fd12dde/5519e33de4b06a1002802805/1431718693701/?format=1500w">
				<m:MaterialImage url="http://b.vimeocdn.com/ps/339/488/3394886_300.jpg" />
				<m:MaterialLabel text="GWT Material" textColor="WHITE" />
				<m:MaterialLink ui:field="pagetitle" 
					activates="dropProfile" iconType="ARROW_DROP_DOWN" iconPosition="RIGHT"
					textColor="WHITE" />
			</ma:sideprofile.MaterialSideProfile>
			<m:MaterialLink ui:field="interconnect" />
			<m:MaterialLink ui:field="devicedeclaration" />
			<m:MaterialLink ui:field="explore" />
			<m:MaterialLink ui:field="cleardb" />
			<m:MaterialLink ui:field="readinit" />
			<m:MaterialLink ui:field="users" />
			<m:MaterialLink ui:field="organizations" />
		</m:MaterialSideNav>

		<m:MaterialContainer>
			<m:MaterialRow textAlign="CENTER" paddingTop="120">
				<m:MaterialPanel ui:field="content"/> 
				<m:MaterialFAB>
					<m:MaterialButton type="FLOATING"
						href="https://github.com/GwtMaterialDesign/gwt-material-patterns/blob/release_2.0/src/main/java/com/github/gwtmaterialdesign/client/application/sidenavfixed/FixedSideNavView.ui.xml"
						target="_blank" backgroundColor="PINK" size="LARGE" iconType="CODE" />
				</m:MaterialFAB>
			</m:MaterialRow>
		</m:MaterialContainer>
	</m:MaterialPanel>
</ui:UiBinder>