<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialModal ui:field="modal" type="DEFAULT"
		inDuration="500" outDuration="500" dismissible="true">
		<m:MaterialModalContent ui:field="modalcontent">
			<m:MaterialPanel>
				<m:MaterialRow>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialLink ui:field="title" />
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<ma:tree.MaterialTree ui:field="tree" />
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialPanel>
		</m:MaterialModalContent>
		<m:MaterialModalFooter ui:field="footer">
			<m:MaterialLink ui:field="close" text="CLOSE" type="FLAT"
				iconColor="BLACK" textAlign="RIGHT"/>
			<m:MaterialLink ui:field="choosedevice" type="FLAT"
				text="Device" textColor="BLACK" textAlign="LEFT"/>
			<m:MaterialLink ui:field="choosesubsystem" type="FLAT"
				text="Subsystem" textColor="BLACK" />
			<m:MaterialLink ui:field="choosecomponent" type="FLAT"
				text="Component" textColor="BLACK" />
		</m:MaterialModalFooter>
	</m:MaterialModal>

</ui:UiBinder> 