<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}
	</ui:style>
			<m:MaterialRow>
				<m:MaterialColumn grid="s1" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="arrowup" iconType="ARROW_DROP_DOWN" iconColor="BLACK"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1">
					<m:MaterialLink ui:field="arrowdown" iconType="ARROW_DROP_UP" iconColor="BLACK"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s9" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="title" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="delete" iconType="DELETE" iconColor="BLACK"/>
				</m:MaterialColumn>
			</m:MaterialRow>
</ui:UiBinder> 
