<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:m.table='urn:import:gwt.material.design.client.ui.table'>
	<ui:style>
	.box {
		background-color: #bdbdbd;
		border: 3px solid #607d8b;
		margin: 0px;
	}
	.panelbox {
		background-color: #e0e0e0;
		border: 3px solid #607d8b;
		margin: 0px;
	}
		.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}	
	
	</ui:style>
	<m:MaterialPanel styleName="{style.panelbox}">
		<m:MaterialRow ui:field="headerrow">
			<m:MaterialColumn grid="s12">
				<m:MaterialLink ui:field="headername" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="blockdefinition">
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="startrow" styleName="{style.rowbox}" textColor="BLACK" text="0"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="endrow" styleName="{style.rowbox}" textColor="BLACK" text="0"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="startcolumn" styleName="{style.rowbox}" textColor="BLACK" text="0"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="endcolumn" styleName="{style.rowbox}" textColor="BLACK" text="0"/>
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow styleName="{style.rowbox}">
			<m:MaterialColumn grid="s11">
				<m:MaterialLink ui:field="corrspecheader" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="addcorr" iconType="ADD" iconColor="BLACK"/>
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="corrspecpanelrow">
			<m:MaterialColumn grid="s12">
				<m:MaterialPanel ui:field="corrspecpanel" />
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder>
