<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" 
	xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:c="urn:import:gwt.material.design.addins.client.combobox">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel ui:field="toppanel" padding="0">
	<m:MaterialPanel ui:field="modalpanel" padding="0"/>
		<m:MaterialRow>
			<m:MaterialColumn grid="s1">
				<m:MaterialTooltip ui:field="tip">
					<m:MaterialIcon ui:field="info" iconType="INFO" iconColor="BLACK" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
					<m:MaterialLink ui:field="label"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s4">
				<m:MaterialTooltip ui:field="purposetip">
					<m:MaterialLink ui:field="purpose"/>
					</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="concepttip">
				<m:MaterialLink ui:field="concept" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 