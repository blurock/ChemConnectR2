
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">

	<ui:with field='res'
		type='info.esblurock.reaction.chemconnect.core.client.resources.ImageResources' />

	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="GREY">
			<m:MaterialTooltip ui:field="identifiertip">
				<m:MaterialLink ui:field="specification"
					textColor="BLACK" />
			</m:MaterialTooltip>
			<m:MaterialLink ui:field="delete" iconType="CLOSE"
				iconPosition="RIGHT" />
			<m:MaterialLink ui:field="upload" iconType="CLOUD_UPLOAD"
				iconPosition="RIGHT" />
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody> 
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<m:MaterialCollapsible ui:field="collapsible" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 
