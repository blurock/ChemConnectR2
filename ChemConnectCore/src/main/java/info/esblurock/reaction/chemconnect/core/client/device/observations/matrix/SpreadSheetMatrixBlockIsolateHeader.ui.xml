<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialPanel>
			<m:MaterialRow>
				<m:MaterialColumn grid="s3" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="startrow" textColor="BLACK"
						text="Row Start" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s5" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="startrowtype" textColor="BLACK"
						text="chose type" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="startrowtext" textColor="BLACK"
						text="0" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s3" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="endrow" textColor="BLACK"
						text="Row End" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s5" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="endrowtype" textColor="BLACK"
						text="chose type" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="endrowtext" textColor="BLACK"
						text="0" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s3" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="startcolumn" textColor="BLACK"
						text="Column Start" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s5" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="startcolumntype"
						textColor="BLACK" text="chose type" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="startcolumnext" textColor="BLACK"
						text="0" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s3" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="endcolumn" textColor="BLACK"
						text="Column End" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s5" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="endcolumntype" textColor="BLACK"
						text="chose type" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
					<m:MaterialLink ui:field="endcolumntext" textColor="BLACK"
						text="0" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialPanel>
		<m:MaterialCollapsible>
			<m:MaterialCollapsibleItem>
				<m:MaterialCollapsibleHeader>
					<m:MaterialTooltip ui:field="originaltooltip"
						text="The reference matrix (the pattern on which to base the block)">
						<m:MaterialLink ui:field="originalmatrix" text="Reference Matrix"
							textColor="BLACK" />
					</m:MaterialTooltip>
				</m:MaterialCollapsibleHeader>
				<m:MaterialCollapsibleBody>
					<m:MaterialPanel ui:field="originalmatrixpanel" />
				</m:MaterialCollapsibleBody>
			</m:MaterialCollapsibleItem>
			<m:MaterialCollapsibleItem>
				<m:MaterialCollapsibleHeader>
					<m:MaterialRow>
						<m:MaterialColumn grid="s10">
							<m:MaterialLink ui:field="block" text="Sub-Block Matrix" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s2">
							<m:MaterialLink ui:field="apply" iconColor="BLACK"
								iconType="REFRESH" />
						</m:MaterialColumn>
					</m:MaterialRow>
					<m:MaterialLink ui:field="interpreted" textColor="BLACK" />
				</m:MaterialCollapsibleHeader>
				<m:MaterialCollapsibleBody>
					<m:MaterialPanel ui:field="blockmatrixpanel" />
				</m:MaterialCollapsibleBody>
			</m:MaterialCollapsibleItem>
		</m:MaterialCollapsible>
	</m:MaterialPanel>
</ui:UiBinder> 