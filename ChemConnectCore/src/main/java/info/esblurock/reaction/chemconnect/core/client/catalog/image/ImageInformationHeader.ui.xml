<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
		margin: 0px
	}
	</ui:style>
		<m:MaterialPanel>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12" styleName="{style.rowbox}">
					<m:MaterialTooltip ui:field="typetooltip">
					<m:MaterialLink ui:field="type" />
					</m:MaterialTooltip>
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12" >
				<m:MaterialPanel ui:field="imagePanel"/>
				</m:MaterialColumn>
				</m:MaterialRow>
		</m:MaterialPanel>
</ui:UiBinder> 