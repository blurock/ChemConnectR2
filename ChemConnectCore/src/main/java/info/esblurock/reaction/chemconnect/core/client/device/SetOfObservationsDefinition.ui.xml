<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialPanel ui:field="modalpanel" />
		<m:MaterialRow>
			<m:MaterialColumn grid="s4">
				<m:MaterialLink ui:field="choose" textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s8">
				<m:MaterialTextBox ui:field="topname" label="Catagory"
					textColor="BLACK" />
			</m:MaterialColumn>
		</m:MaterialRow>

		<m:MaterialCollapsible>
			<m:MaterialCollapsibleItem>
				<m:MaterialCollapsibleHeader>
					<m:MaterialLink ui:field="parameter" textColor="BLACK" />
				</m:MaterialCollapsibleHeader>
				<m:MaterialCollapsibleBody ui:field="bodycollapsible">
					<m:MaterialCollapsible ui:field="contentcollapsible">
					</m:MaterialCollapsible>
				</m:MaterialCollapsibleBody>
			</m:MaterialCollapsibleItem>
		</m:MaterialCollapsible>
	</m:MaterialPanel>
</ui:UiBinder> 