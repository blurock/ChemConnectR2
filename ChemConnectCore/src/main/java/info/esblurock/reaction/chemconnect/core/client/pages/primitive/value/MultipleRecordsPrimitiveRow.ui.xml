<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:c="urn:import:gwt.material.design.addins.client.combobox">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialRow>
			<m:MaterialColumn grid="s9">
				<m:MaterialLink ui:field="parameterType" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="info" iconType="INFO"
					iconPosition="RIGHT" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="clear" iconType="CLEAR"
					iconPosition="RIGHT" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="add" iconType="ADD"
					iconPosition="RIGHT" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialPanel ui:field="content" />
		</m:MaterialRow>
		<m:MaterialDivider/>		
	</m:MaterialPanel>
</ui:UiBinder> 