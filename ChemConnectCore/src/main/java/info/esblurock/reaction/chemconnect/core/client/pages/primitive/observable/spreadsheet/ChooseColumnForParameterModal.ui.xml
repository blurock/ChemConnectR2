<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialDialog ui:field="modal" type="DEFAULT"
		inDuration="500" outDuration="500" dismissible="true" height="200px">
		<m:MaterialDialogContent ui:field="modalcontent">
			<m:MaterialPanel>
				<m:MaterialRow backgroundColor="GREY_LIGHTEN_2">
					<m:MaterialColumn grid="s6">
						<m:MaterialLink ui:field="parameter" textColor="BLACK" backgroundColor="GREY_LIGHTEN_2"/>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s6">
						<m:MaterialLink ui:field="button" backgroundColor="GREY_LIGHTEN_2"
							text="" iconType="ARROW_DROP_DOWN"
							iconPosition="RIGHT" textColor="BLACK" />
						<m:MaterialDropDown ui:field="column" activator="dp-2"
							constrainWidth="true" />
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialPanel>
		</m:MaterialDialogContent>
		<m:MaterialDialogFooter ui:field="footer"
			paddingTop="0" paddingBottom="0">
			<m:MaterialLink ui:field="close" text="CLOSE" type="FLAT"
				iconColor="BLACK" textAlign="RIGHT" />
		</m:MaterialDialogFooter>
	</m:MaterialDialog>

</ui:UiBinder> 