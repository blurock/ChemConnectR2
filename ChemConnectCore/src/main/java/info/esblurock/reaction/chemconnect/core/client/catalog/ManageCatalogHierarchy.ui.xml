<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.panelbox {
		background-color: #e0e0e0;
		border: 2px solid #607d8b;
		margin: 0px;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialPanel ui:field="modalpanel" />
		<m:MaterialPanel margin="0">
			<m:MaterialRow>
				<m:MaterialColumn grid="s10">
					<m:MaterialTooltip ui:field="identifiertip">
						<m:MaterialTitle ui:field="title" textColor="BLACK" />
					</m:MaterialTooltip>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1">
					<m:MaterialIcon ui:field="save" iconColor="BLACK"
						iconType="SAVE" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1">
					<m:MaterialIcon ui:field="refresh" iconColor="BLACK"
						iconType="REFRESH" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialPanel>
		<m:MaterialCollapsible ui:field="panel" styleName="{style.panelbox}"/>
	</m:MaterialPanel>
</ui:UiBinder> 
