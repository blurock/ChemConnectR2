<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialPanel ui:field="modalpanel" />
		<m:MaterialPanel>
			<m:MaterialRow>
				<m:MaterialColumn grid="s11">
					<m:MaterialTooltip ui:field="identifiertip">
						<m:MaterialTitle ui:field="title" textColor="BLACK" />
					</m:MaterialTooltip>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1">
					<m:MaterialIcon ui:field="refresh" iconColor="BLACK"
						iconType="REFRESH" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialPanel>
		<m:MaterialCollapsible ui:field="panel" />
	</m:MaterialPanel>
</ui:UiBinder> 
