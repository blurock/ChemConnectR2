<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:c="urn:import:gwt.material.design.addins.client.combobox">
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	.thinline {
		font-weight: bold;
		color: red;
		text-color: red;
		text-align: left;
		height: 10px;
		background-color: red;
	}
	</ui:style>
	<m:MaterialPanel ui:field="toppanel" paddingBottom="0"
		paddingTop="0" border="3px solid #e9e9e9">
		<m:MaterialPanel ui:field="modalpanel" />
		<m:MaterialRow paddingBottom="0" paddingTop="0">
			<m:MaterialColumn ui:field="labelcolumn" grid="s3">
				<m:MaterialTooltip ui:field="identifiertip">
					<m:MaterialLink ui:field="parameterLabel"
						textColor="BLACK" height="10px" textAlign="LEFT" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="valuecolumn" grid="s3">
				<m:MaterialTooltip ui:field="valuetip">
					<m:MaterialLink ui:field="valueTextBox"
						class="thinline" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="uncertaintycolumn"
				grid="s3">
				<m:MaterialTooltip ui:field="uncertaintytip">
					<m:MaterialLink ui:field="uncertaintyTextBox"
						height="10px" textColor="BLACK" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="unitscolumn" grid="s2">
				<m:MaterialTooltip ui:field="unitstip">
					<m:MaterialLink ui:field="parameterUnits"
						height="10px" textColor="BLACK" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="more" height="10px"
					iconType="EXPAND_MORE" />
				<m:MaterialLink ui:field="less" height="10px"
					iconType="EXPAND_LESS" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="extrainfo" paddingBottom="0"
			paddingTop="0">
			<m:MaterialColumn grid="s3">
				<m:MaterialTooltip ui:field="purposetip">
					<m:MaterialLink ui:field="purpose" textColor="BLACK"
						height="10px" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialTooltip ui:field="concepttip">
					<m:MaterialLink ui:field="concept" textColor="BLACK"
						height="10px" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialTooltip ui:field="uncertaintyclasstip">
					<m:MaterialLink ui:field="uncertaintyclass"
						textColor="BLACK" height="10px" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn ui:field="unitsclasscolumn"
				grid="s2">
				<m:MaterialTooltip ui:field="unitsclasstip">
					<m:MaterialLink ui:field="unitclass"
						textColor="BLACK" height="10px" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialTooltip ui:field="deletetip">
					<m:MaterialLink ui:field="delete" height="10px"
						iconType="CLEAR" />
				</m:MaterialTooltip>
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 