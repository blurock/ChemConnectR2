<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel ui:field="panel">
		<m:MaterialRow>
			<m:MaterialColumn grid="s4">
				<m:MaterialButton ui:field="getCoordinates" text="Get Coordinates" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s4">
				<m:MaterialTextBox ui:field="gpslatitude" text="55.7 N"
					placeholder="Latitude" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s4">
				<m:MaterialTextBox ui:field="gpslongitude" text="13.2 E"
					placeholder="Longitude" />
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 