<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:c="urn:import:gwt.material.design.addins.client.combobox">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
	<m:MaterialPanel ui:field="modalpanel"/>
	<m:MaterialRow>
		<m:MaterialColumn grid="s6">
			<m:MaterialLink ui:field="unitclass" textColor="BLACK" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s6">
			<m:MaterialTextBox ui:field="unitsTextBox"
				textColor="BLACK" />
			<c:MaterialComboBox ui:field="parameterUnits" />
		</m:MaterialColumn>
	</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 