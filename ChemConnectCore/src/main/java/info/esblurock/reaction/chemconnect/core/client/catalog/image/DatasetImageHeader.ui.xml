<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
		margin: 0px
	}
	</ui:style>
		<m:MaterialPanel>
			<m:MaterialRow>
				<m:MaterialColumn grid="s10" styleName="{style.rowbox}">
					<m:MaterialTooltip ui:field="headertooltip">
					<m:MaterialLink ui:field="header" />
					</m:MaterialTooltip>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1" >
					<m:MaterialLink ui:field="delete" iconColor="BLACK" iconType="DELETE" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1" >
					<m:MaterialLink ui:field="save" iconColor="BLACK" iconType="SAVE" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialPanel>
</ui:UiBinder> 