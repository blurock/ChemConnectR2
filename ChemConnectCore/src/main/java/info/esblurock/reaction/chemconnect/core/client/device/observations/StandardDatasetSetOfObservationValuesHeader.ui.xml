<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialRow>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="observationtooltip">
				<m:MaterialLink ui:field="observationtype" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s5">
			<m:MaterialTooltip ui:field="titletooltip">
			<m:MaterialLink ui:field="observationhead" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
		<m:MaterialTooltip ui:field="attachtooltip">
			<m:MaterialIcon ui:field="attach" iconType="ATTACH_FILE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
				</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialIcon ui:field="save" iconType="SAVE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialIcon ui:field="delete" iconType="DELETE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>

</ui:UiBinder> 