<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.columnbox {
		background-color: #cfd8dc;
		margin: 3px;
		width: 100%;
	}
	
	.box {
		color: black;
		margin: 0px;
	}
	
	.rowbox {
		background-color: #cfd8dc;
		margin: 0px;
		width: 100%;
		shadow: 3
	}
	
	.panelbox {
		border: 3px solid #607d8b;
		background-color: white;
		margin: 3px;
	}
	
	.button {
		color: black;
		background-color: #cfd8dc;
		margin: 0px;
		width: 100%;
	}
	</ui:style>
	<m:MaterialRow styleName="{style.rowbox}">
		<m:MaterialColumn grid="s6" margin="0">
		<m:MaterialTooltip ui:field="title">
			<m:MaterialLink ui:field="givenname" styleName="{style.button}"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s6" margin="0">
			<m:MaterialTooltip ui:field="contactLink">
				<m:MaterialLink ui:field="familyname" styleName="{style.button}"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
</ui:UiBinder> 