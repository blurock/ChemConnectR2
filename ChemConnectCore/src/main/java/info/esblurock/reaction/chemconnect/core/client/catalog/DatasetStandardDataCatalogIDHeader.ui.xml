<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.panelbox {
		background-color: #f5f5f5;
		border: 3px solid #607d8b;
		margin: 0px;
	}
	.headertext {
		color: black;
		font-style: bold;
		font-size:"200%";
		margin: 0pt;
	}
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}
	</ui:style>
	<m:MaterialPanel styleName="{style.panelbox}">
	<m:MaterialRow margin="0">
		<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="basetooltip">
			<m:MaterialLink ui:field="catalogBase" styleName="{style.headertext}"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
			<m:MaterialLink ui:field="catalog" styleName="{style.headertext}"/>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
			<m:MaterialLink ui:field="simpleName" styleName="{style.headertext}"/>
		</m:MaterialColumn>
	</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 