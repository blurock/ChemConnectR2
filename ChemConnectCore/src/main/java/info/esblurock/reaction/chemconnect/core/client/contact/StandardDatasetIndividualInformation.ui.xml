<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialRow>
		<m:MaterialColumn grid="s9">
			<m:MaterialTooltip ui:field="persontooltip">
			<m:MaterialLink ui:field="personhead" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="delete" iconType="DELETE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="save" iconType="SAVE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>

</ui:UiBinder> 