	xmlns:m="urn:import:gwt.material.design.client.ui" 
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	
	</ui:style>
	<m:MaterialCollapsibleItem ui:field="body">
		<m:MaterialCollapsibleHeader>
			<m:MaterialRow>
				<m:MaterialColumn grid="s11">
					<m:MaterialLink ui:field="datatype" text="Record" textColor="BLACK" />
				</m:MaterialColumn>
				<m:MaterialColumn ui:field="infocolumn" grid="s1">
