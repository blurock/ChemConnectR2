<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	</ui:style>
		<m:MaterialCollapsibleItem ui:field="body">
			<m:MaterialCollapsibleHeader ui:field="infoheader">
			<m:MaterialRow>
				<m:MaterialColumn grid="s5">
					<m:MaterialLabel ui:field="datatype" textColor="BLACK" />
				</m:MaterialColumn>
				<m:MaterialColumn ui:field="columnlabel" grid="s5">
					<m:MaterialLabel ui:field="identifier" textColor="BLACK" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1">
					<m:MaterialLink ui:field="info" iconType="INFO"
						iconPosition="RIGHT" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s1">
					<m:MaterialLink ui:field="expand" iconPosition="RIGHT" />
				</m:MaterialColumn>
			</m:MaterialRow>
			</m:MaterialCollapsibleHeader>
			<m:MaterialCollapsibleBody>
				<m:MaterialPanel ui:field="panel"/>
			</m:MaterialCollapsibleBody>
		</m:MaterialCollapsibleItem>
</ui:UiBinder> 