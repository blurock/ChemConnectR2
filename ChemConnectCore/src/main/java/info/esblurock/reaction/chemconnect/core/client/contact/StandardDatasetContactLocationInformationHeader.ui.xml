<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
	<m:MaterialRow>
		<m:MaterialColumn grid="s10">
			<m:MaterialTooltip ui:field="addresstooltip">
				<m:MaterialLink ui:field="address" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="delete" iconType="DELETE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="save" iconType="SAVE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="citytooltip">
				<m:MaterialLink ui:field="city" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s3">
			<m:MaterialTooltip ui:field="countrytooltip">
				<m:MaterialLink ui:field="country" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s3">
			<m:MaterialTooltip ui:field="postcodetooltip">
				<m:MaterialLink ui:field="postcode" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
</m:MaterialPanel>
</ui:UiBinder> 