<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel  padding="0" paddingTop="0" paddingBottom="0">
	<m:MaterialRow  padding="0" paddingTop="0" paddingBottom="0">
		<m:MaterialColumn grid="s12" padding="0" paddingTop="0" paddingBottom="0">
			<m:MaterialTooltip ui:field="addresstooltip">
				<m:MaterialLink ui:field="address" textColor="BLACK"  padding="0" paddingTop="0" paddingBottom="0"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow  padding="0" paddingTop="0" paddingBottom="0">
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="citytooltip">
				<m:MaterialLink ui:field="city" textColor="BLACK"  padding="0" paddingTop="0" paddingBottom="0"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="countrytooltip">
				<m:MaterialLink ui:field="country" textColor="BLACK"  padding="0" paddingTop="0" paddingBottom="0"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="postcodetooltip">
				<m:MaterialLink ui:field="postcode" textColor="BLACK"  padding="0" paddingTop="0" paddingBottom="0"/>
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
</m:MaterialPanel>
</ui:UiBinder> 