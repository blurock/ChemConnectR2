<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
xmlns:g="urn:import:com.google.gwt.user.client.ui" 
xmlns:m="urn:import:gwt.material.design.client.ui">

	<ui:style>
	.box {
		background-color: #bdbdbd;
		border: 3px solid #607d8b;
		margin: 0px;
	}
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}
	
	</ui:style>
		<m:MaterialPanel styleName="{style.box}">
	<m:MaterialRow margin="0">
		<m:MaterialColumn grid="s4" styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="linktypetooltip">
			<m:MaterialLink ui:field="linktype" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s7"  styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="httptooltip">
			<m:MaterialLink ui:field="httpaddress" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1"  styleName="{style.rowbox}">
			<m:MaterialLink ui:field="delete" iconType="DELETE" iconColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 
