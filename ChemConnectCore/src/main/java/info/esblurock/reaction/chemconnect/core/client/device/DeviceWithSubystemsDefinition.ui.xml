<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
	<m:MaterialPanel ui:field="modalpanel"/>
	<m:MaterialRow>
	<m:MaterialColumn grid="s12">
	<m:MaterialLink ui:field="choose" text="Device Subsystem Component" textColor="BLACK"/>
	</m:MaterialColumn>
	</m:MaterialRow>
	
	<m:MaterialCollapsible>
	<m:MaterialCollapsibleItem>
			<m:MaterialCollapsibleHeader>
				<m:MaterialLink ui:field="parameter" textColor="BLACK" />
			</m:MaterialCollapsibleHeader>
			<m:MaterialCollapsibleBody ui:field="bodycollapsible">
				<m:MaterialCollapsible ui:field="contentcollapsible"/>
			</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
	</m:MaterialCollapsible>
	</m:MaterialPanel>
</ui:UiBinder> 
