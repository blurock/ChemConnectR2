<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.box {
		background-color: #bdbdbd;
		border: 1px solid #607d8b;
		margin: 0px;
	}
	</ui:style>
	<m:MaterialModal ui:field="modal" type="DEFAULT"
		inDuration="500" outDuration="500" dismissible="true" height="200px">
		<m:MaterialModalContent ui:field="modalcontent">
			<m:MaterialPanel>
				<m:MaterialRow styleName="{style.box}">
					<m:MaterialColumn grid="s1">
						<m:MaterialCheckBox ui:field="databasecheckbox" />
					</m:MaterialColumn>
					<m:MaterialColumn grid="s11">
						<m:MaterialLabel ui:field="databasetextbox" />
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow styleName="{style.box}">
					<m:MaterialColumn grid="s1">
						<m:MaterialCheckBox ui:field="yamlcheckbox" />
					</m:MaterialColumn>
					<m:MaterialColumn grid="s11">
						<m:MaterialLabel ui:field="yamltextbox" />
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialPanel>
		</m:MaterialModalContent>
		<m:MaterialModalFooter ui:field="footer" >
			<m:MaterialLink ui:field="close" text="CLOSE" type="FLAT"
				iconColor="BLACK" textAlign="RIGHT"/>		
			<m:MaterialLink ui:field="done" text="Done" type="FLAT"
				iconColor="BLACK" textAlign="RIGHT"/>		
		</m:MaterialModalFooter>
	</m:MaterialModal>

</ui:UiBinder> 