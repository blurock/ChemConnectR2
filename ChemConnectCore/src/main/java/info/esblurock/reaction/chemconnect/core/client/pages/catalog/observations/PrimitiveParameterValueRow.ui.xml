<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:c="urn:import:gwt.material.design.addins.client.combobox">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel ui:field="toppanel">
		<m:MaterialPanel ui:field="modalpanel" />
		<m:MaterialRow>
			<m:MaterialColumn grid="s3">
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialTooltip ui:field="identifiertip">
							<m:MaterialLink ui:field="parameterLabel" textColor="BLACK" />
						</m:MaterialTooltip>
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s6">
						<m:MaterialTooltip ui:field="typetip">
							<m:MaterialSwitch ui:field="toggleinfo" />
						</m:MaterialTooltip>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s6">
						<m:MaterialLink ui:field="clear" iconColor="BLACK"
							iconType="CLEAR" />
					</m:MaterialColumn>
				</m:MaterialRow>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialTextBox ui:field="valueTextBox"
					textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialTextBox ui:field="uncertaintyTextBox"
					textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialTextBox ui:field="unitsTextBox"
					textColor="BLACK" />
				<c:MaterialComboBox ui:field="parameterUnits" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow ui:field="extrainfo">
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="purpose" textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="concept" textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="unitclass" textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialLink ui:field="uncertaintyclass"
					textColor="BLACK" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialDivider />
	</m:MaterialPanel>
</ui:UiBinder> 