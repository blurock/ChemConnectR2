<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
	<m:MaterialRow>
		<m:MaterialColumn grid="s5">
			<m:MaterialTooltip ui:field="orgnametooltip">
				<m:MaterialLink ui:field="orgname" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s5">
			<m:MaterialTooltip ui:field="orgclasstooltip">
				<m:MaterialLink ui:field="orgclass" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="delete" iconType="DELETE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="save" iconType="SAVE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow>
		<m:MaterialColumn grid="s6">
			<m:MaterialTooltip ui:field="suborgtooltip">
				<m:MaterialLink ui:field="suborg" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s6">
			<m:MaterialTooltip ui:field="orgunittooltip">
				<m:MaterialLink ui:field="orgunit" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
</m:MaterialPanel>
</ui:UiBinder> 