<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.box {
		background-color: #bdbdbd;
		border: 3px solid #607d8b;
		margin: 0px;
	}
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		width: 95%;
	}
	
	</ui:style>
	<m:MaterialPanel styleName="{style.box}">
	<m:MaterialRow margin="0">
		<m:MaterialColumn grid="s6" styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="orgnametooltip">
				<m:MaterialLink ui:field="orgname" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s6" styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="orgclasstooltip">
				<m:MaterialLink ui:field="orgclass" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow margin="0">
		<m:MaterialColumn grid="s6" styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="suborgtooltip">
				<m:MaterialLink ui:field="suborg" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s6" styleName="{style.rowbox}">
			<m:MaterialTooltip ui:field="orgunittooltip">
				<m:MaterialLink ui:field="orgunit" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
</m:MaterialPanel>
</ui:UiBinder> 