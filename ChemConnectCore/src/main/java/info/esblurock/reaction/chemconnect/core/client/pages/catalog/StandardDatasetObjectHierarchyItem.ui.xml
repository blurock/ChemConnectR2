	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader ui:field="header" />
		<m:MaterialCollapsibleBody  ui:field="topbody" backgroundColor="WHITE">
			<m:MaterialCollapsible ui:field="subinfo">
