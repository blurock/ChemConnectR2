<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.rowbox {
		background-color: #cfd8dc;
		border: 1px solid #607d8b;
		margin: 0px;
	}
	</ui:style>
	<m:MaterialRow margin="0">
		<m:MaterialColumn ui:field="labelCountRow" grid="s2">
			<m:MaterialLink ui:field="labelCount" textColor="BLACK" text="  "/>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s9">
			<m:MaterialLink ui:field="specificationLabel" textColor="BLACK"/>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1" >
			<m:MaterialTooltip ui:field="uncertaintytooltip">
				<m:MaterialCheckBox ui:field="uncertainty" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
</ui:UiBinder> 
