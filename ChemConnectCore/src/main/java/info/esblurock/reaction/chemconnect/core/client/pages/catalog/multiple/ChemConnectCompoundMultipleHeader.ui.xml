<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style field="header">
	.panelbox {
		background-color: #f5f5f5;
		border: 3px solid #607d8b;
		margin: 0px;
	}
	.headertext {
		color: black;
		font-style: bold;
		font-size:"110%";
		align: right;
	}
	</ui:style>
	<m:MaterialPanel styleName="{header.panelbox}">
		<m:MaterialRow>
			<m:MaterialColumn grid="s5">
				<m:MaterialLink ui:field="multipletitle" styleName="{header.headertext}"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s6">
				<m:MaterialLink ui:field="typename"  styleName="{header.headertext}"/>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s1">
				<m:MaterialLink ui:field="add" iconType="ADD"
					iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialRow margin="0">
			<m:MaterialPanel ui:field="multiobjects" margin="0" />
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 