<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialPanel ui:field="modalpanel" />
					<m:MaterialRow border="3px solid #e9e9e9">
						<m:MaterialColumn grid="s1">
						<m:MaterialLink ui:field="titlelabel" textColor="BLACK" />
						</m:MaterialColumn>
						<m:MaterialColumn grid="s11">
							<m:MaterialTooltip ui:field="titletooltip">
								<m:MaterialLink ui:field="title" textColor="BLACK" />
							</m:MaterialTooltip>
						</m:MaterialColumn>
					</m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialColumn grid="s6" >
				<m:MaterialPanel border="3px solid #e9e9e9">
					<m:MaterialRow>
						<m:MaterialColumn grid="s6">
							<m:MaterialTooltip ui:field="ownertooltip">
								<m:MaterialLink ui:field="owner" textColor="BLACK" />
							</m:MaterialTooltip>
						</m:MaterialColumn>
						<m:MaterialColumn grid="s6">
							<m:MaterialTooltip ui:field="accessibiltytooltip">
								<m:MaterialLink ui:field="accessibilty"
									textColor="BLACK" />
							</m:MaterialTooltip>
						</m:MaterialColumn>
					</m:MaterialRow>
					<m:MaterialDivider/>
					<m:MaterialRow>
						<m:MaterialColumn grid="s12">
							<m:MaterialTooltip ui:field="datetooltip">
								<m:MaterialDatePicker ui:field="date"
									selectionType="YEAR"  width="100%"/>
							</m:MaterialTooltip>
						</m:MaterialColumn>
					</m:MaterialRow>
				</m:MaterialPanel>
			</m:MaterialColumn>
			<m:MaterialColumn grid="s6">
				<m:MaterialTextArea ui:field="abstractText"
					textColor="BLACK" height="100%"  border="3px solid #e9e9e9"/>
			</m:MaterialColumn>
		</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 