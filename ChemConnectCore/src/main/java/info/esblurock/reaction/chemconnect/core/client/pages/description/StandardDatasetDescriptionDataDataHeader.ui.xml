<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
	<m:MaterialPanel ui:field="modalpanel"/>
	<m:MaterialRow>
		<m:MaterialColumn grid="s11">
		<m:MaterialLink ui:field="titlelabel" textColor="BLACK" />
			<m:MaterialTooltip ui:field="titletooltip">
				<m:MaterialLink ui:field="title" textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s1">
			<m:MaterialLink ui:field="save" iconType="SAVE"
				iconColor="BLACK" iconPosition="RIGHT" textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow>
		<m:MaterialColumn grid="s12">
			<m:MaterialTextArea ui:field="abstractText"  textColor="BLACK" />
		</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="ownertooltip">
				<m:MaterialLink ui:field="owner"  textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="accessibiltytooltip">
				<m:MaterialLink ui:field="accessibilty"  textColor="BLACK" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
		<m:MaterialColumn grid="s4">
			<m:MaterialTooltip ui:field="datetooltip">
				<m:MaterialDatePicker ui:field="date"
					selectionType="YEAR" />
			</m:MaterialTooltip>
		</m:MaterialColumn>
	</m:MaterialRow>
	</m:MaterialPanel>
</ui:UiBinder> 