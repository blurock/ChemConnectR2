<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
		<m:MaterialRow ui:field="titlerow">
			<m:MaterialColumn grid="s2">
				<m:MaterialLink ui:field="attribute" textColor="BLACK" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s10">
				<m:MaterialLink ui:field="title"
					textColor="BLACK" />
			</m:MaterialColumn>
		</m:MaterialRow>
</ui:UiBinder> 