<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">



  <servlet>
    <servlet-name>gcsfileupload</servlet-name>
    <servlet-class>info.esblurock.reaction.core.server.db.gcs.FileUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>gcsfileupload</servlet-name>
    <url-pattern>/gcsfileupload</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>fileupload</servlet-name>
    <servlet-class>info.esblurock.reaction.core.server.db.image.UploadImageService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fileupload</servlet-name>
    <url-pattern>/fileupload</url-pattern>
  </servlet-mapping>
  	<servlet>
		<servlet-name>images</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.db.image.UserImageServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>images</servlet-name>
		<url-pattern>/chemconnectcore/images</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>uploadServlet</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.db.image.UploadServlet</servlet-class>
	</servlet>
		<servlet-mapping>
		<servlet-name>uploadServlet</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
  
  
  
  
  
  <servlet>
    <servlet-name>initialization</servlet-name>
    <servlet-class>info.esblurock.reaction.core.server.services.InitalizationServiceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>initialization</servlet-name>
    <url-pattern>/chemconnectcore/initialization</url-pattern>
  </servlet-mapping>
  
  	<servlet>
		<servlet-name>loginservice</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.authentification.LoginServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginservice</servlet-name>
		<url-pattern>/chemconnectcore/loginservice</url-pattern>
		<url-pattern>/loginservice</url-pattern>
	</servlet-mapping>
  	<servlet>
		<servlet-name>contactservice</servlet-name>
		<servlet-class>info.esblurock.reaction.core.server.services.ContactDatabaseAccessImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>contactservice</servlet-name>
		<url-pattern>/chemconnectcore/contactservice</url-pattern>
	</servlet-mapping>
  
  <filter>
	<filter-name>ObjectifyFilter</filter-name>
	<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
</filter>
<filter-mapping>
	<filter-name>ObjectifyFilter</filter-name>
	<url-pattern>/*</url-pattern>
</filter-mapping>
<listener>
  <listener-class>info.esblurock.reaction.core.server.db.DatabaseRegistry</listener-class>
</listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>